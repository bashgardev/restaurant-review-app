{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "react-hooks/exhaustive-deps": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
